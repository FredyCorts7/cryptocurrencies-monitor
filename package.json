  "bugs": {
    "url": "https://github.com/FredyCorts7/cryptocurrencies-monitor/issues"
  },
  "homepage": "https://github.com/FredyCorts7/cryptocurrencies-monitor#readme",
  "devDependencies": {
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "jest": "^26.6.3",
    "mysql2": "^2.2.5",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "sequelize-cli": "^6.2.0",
    "supertest": "^6.1.3"
  },
  "dependencies": {
    "awilix": "^4.3.3",
    "bcrypt": "^5.0.1",
    "coingecko-api": "^1.0.10",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-validator": "^6.10.0",
    "helmet": "^4.4.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.5.0"
  }
}
