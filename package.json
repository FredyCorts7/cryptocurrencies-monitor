{
  "name": "cryptocurrencies-monitor",
  "version": "1.0.0",
  "description": "API para el monitoreo de criptomonedas",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "start:dev": "nodemon index.js",
    "db:init": "sequelize-cli init",
    "db:create": "sequelize-cli db:create",
    "db:migrate:gen": "sequelize-cli migration:create --name",
    "db:migrate": "sequelize-cli db:migrate:undo:all && sequelize-cli db:migrate",
    "db:revert": "sequelize-cli db:migrate:undo"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:FredyCorts7/cryptocurrencies-monitor.git"
  },
  "author": "Fredy Cort√©s",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/FredyCorts7/cryptocurrencies-monitor/issues"
  },
  "homepage": "https://github.com/FredyCorts7/cryptocurrencies-monitor#readme",
  "devDependencies": {
    "dotenv": "^8.2.0",
    "nodemon": "^2.0.7",
    "sequelize-cli": "^6.2.0"
  },
  "dependencies": {
    "awilix": "^4.3.3",
    "bcrypt": "^5.0.1",
    "coingecko-api": "^1.0.10",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-validator": "^6.10.0",
    "helmet": "^4.4.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.5.0"
  }
}
