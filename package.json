{
  "name": "cryptocurrencies-monitor",
  "version": "1.0.0",
  "description": "API para el monitoreo de criptomonedas",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "start:dev": "npm-run-all db:migrate dev",
    "start:test": "cross-env NODE_ENV=test npm-run-all db:migrate db:seed:gen test:jest",
    "db:seed:gen": "sequelize-cli db:seed:all",
    "db:migrate": "sequelize-cli db:migrate:undo:all && sequelize-cli db:migrate",
    "test:jest": "jest --verbose",
    "test:coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:FredyCorts7/cryptocurrencies-monitor.git"
  },
  "author": "Fredy Cort√©s",
